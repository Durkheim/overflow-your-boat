<b>Edit <%= @question.title%></b>
last edited at <%= @question.updated_at %>
<%= form_for(@question) do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title%>

      <%= f.label :description %>
      <%= f.text_area :description%>

      <%= f.submit "Submit"%>
      <%=link_to 'Cancel', user_path(current_user)%>
    <% end %>
